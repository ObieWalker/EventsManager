  },
  "description": "testing api",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "author": "Obi Walker",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
