    "fs": "0.0.1-security",
    "history": "^4.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.2.0",
    "immutability-helper": "^2.6.6",
    "jade": "~1.11.0",
    "jest": "^23.0.0",
    "jest-enzyme": "^6.0.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-blacklist": "0.0.2",
    "jwt-decode": "^2.2.0",
