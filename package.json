{
  "name": "event-manager",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run heroku-postbuild && node --optimize_for_size --max_old_space_size=920 --gc_interval=100 ./dist/app.js",
    "build:prod:client": "rm -rf build && webpack --progress --config webpack.config.prod.js",
    "build:prod:server": "babel -d ./dist --copy-files ./server -s",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "start:dev": "export NODE_ENV=development && npm run build:dev && nodemon server/entry.js",
    "build:dev:client": "webpack --progress --config webpack.config.dev.js",
    "start:dev2": "export NODE_ENV=development && nodemon --watch server --exec babel-node -- server/app.js",
    "dev": "webpack-dev-server --progress --colors --content-base=./client/public",
    "build": "babel src -d dist",
    "heroku-postbuild": "npm run build:prod:client && npm run build:prod:server",
    "mocha": "mocha --compilers js:babel-core/register",
    "pretest": "npm run db:migration && NODE_ENV=development",
    "seq:create": "sequelize db:create events_test",
    "test": "NODE_ENV=development nyc --reporter=html --reporter=text mocha --timeout 20000 ./server/tests/index.js --exit --require babel-register --reporter spec",
    "test:dev": "export NODE_ENV=test npm run db:migration:test && nyc --reporter=html --reporter=text mocha --timeout 20000 ./server/tests/index.js --exit --require babel-register --reporter spec",
    "test:client": "jest && nyc --reporter=html --reporter=text",
    "db:migration": "sequelize db:migrate && sequelize db:seed:all",
    "db:migration:test": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "lint": "./node_modules/.bin/eslint",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "jest": {
    "setupFiles": [
      "./client/tests/localStorageMock.js",
      "<rootDir>./client/src/setupTests.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>./client/tests/setups/setupTests.js",
    "testRegex": "client/tests/.*\\.test\\.(js|jsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**",
      "!client/src/index.jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/server/",
      "/build/"
    ],
    "testPathIgnorePatterns": [
      "/client/tests/__mocks__/"
    ]
  },
  "dependencies": {
    "auth-js": "0.0.10",
    "axios": "^0.17.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.2",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "cloudinary-react": "^1.0.6",
    "compression-webpack-plugin": "^1.1.11",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-loader": "^2.0.0",
    "expect": "^23.0.0",
    "express": "^4.15.5",
    "express-validator": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "fbemitter": "^2.1.1",
