    "start": "webpack --mode production && node server/entry.js",
    "build:dev": "webpack --mode development -w",
    "start:dev": "nodemon server/entry.js",
    "build:dev:client": "webpack --progress --config webpack.config.dev.js",
    "start:dev2": "export NODE_ENV=development && nodemon --watch server --exec babel-node -- server/app.js",
    "dev": "webpack-dev-server --progress --colors --content-base=./client/public",
    "build": "babel src -d dist",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "nyc --reporter=html --reporter=text mocha ./server/tests/ --compilers js:babel-register --exit",
    "test:dev": "npm run test && npm run coverage",
    "lint": "./node_modules/.bin/eslint",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
